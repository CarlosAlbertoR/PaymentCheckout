{
    "cli": {
        "version": ">= 10.0.0"
    },
    "build": {
        "preview": {
            "developmentClient": true,
            "distribution": "internal",
            "android": {
                "buildType": "apk"
            },
            "env": {
                "EXPO_PUBLIC_API_URL": "${EXPO_PUBLIC_API_URL}"
            }
        },
        "production": {
            "autoIncrement": true,
            "android": {
                "buildType": "app-bundle"
            },
            "env": {
                "EXPO_PUBLIC_API_URL": "${EXPO_PUBLIC_API_URL}"
            }
        }
    },
    "submit": {
        "production": {}
    }
}